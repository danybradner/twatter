<table>
    <tr>
        <th>screen name </th>
        <th>text</th>
        <th>time</th>
    </tr>
    <% @tweets.each do |tweet| %>
    <tr>
        <td><%= tweet.user.screen_name %> </td>
        <td><%=tweet.text%> </td>
        <td><%= tweet.created_at %> </td>
    </tr>
    <% end %>
</table>